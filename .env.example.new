# SIIMUT Docker Environment
# Copy file ini ke .env dan sesuaikan dengan kebutuhan

APP_NAME=SIIMUT
APP_ENV=staging
APP_DEBUG=true

# Port Settings
APP_PORT=8000
VITE_PORT=5173

# Database Settings
MYSQL_VERSION=10.11
MYSQL_ROOT_PASSWORD=secret
MYSQL_DATABASE=siimut
MYSQL_USER=siimut
MYSQL_PASSWORD=secret
MYSQL_PORT=3306

# Redis Settings
REDIS_PORT=6379

# phpMyAdmin Settings
PMA_PORT=8080

# Git Repository Settings
# Git Repository Settings
SIIMUT_REPO=https://github.com/juniyasyos/si-imut.git
SIIMUT_BRANCH=fix-chart
REPO_NAME=si-imut

# Optional: Private Repository (uncomment jika menggunakan private repo)
# GIT_USERNAME=your_username
# GIT_TOKEN=your_personal_access_token